    function checkform() {

      toastr.options.closeButton = true;
      toastr.options.progressBar = true;

      if (document.contact.fullname.value == '') {
        toastr.error("Please enter your full name!");
        document.contact.fullname.focus();
        return false;
      }


      if (document.contact.email.value == '') {
        toastr.error("Please enter your e-mail address!");
        document.contact.email.focus();
        return false;
      }


      if (document.contact.message.value == '') {
        toastr.error("Please fill in your message");
        document.contact.message.focus();
        return false;
      }


      $.ajax({
        url: "/submit",
        type: "POST",
        dataType: "json",
        contentType: "application/json; charset=utf-8",
        data: JSON.stringify({
          name: $("#name").val(),
          email: $("#email").val(),
          message: $("#message").val()
        }),
        success: function (result) {
          let reply = result.serverResponse;
          toastr.options.closeButton = true;
          toastr.options.progressBar = true;
          toastr.success(reply);
        },
        error: function (err) {
          // check the err for error details
          toastr.options.closeButton = true;
          toastr.options.progressBar = true;
          toastr.error(err.responseText);
        }
      });

    }

    function toaster() {
      toastr.warning("an important info");
    }


  </script>
